<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>



	<servlet>
		<servlet-name>CompanyServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.CompanyServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ListCompanyServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListCompanyServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>DeleteCompanyServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.DeleteCompanyServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>CompanyServlet</servlet-name>
		<url-pattern>/registerCompany</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ListCompanyServlet</servlet-name>
		<url-pattern>/listCompany</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>DeleteCompanyServlet</servlet-name>
		<url-pattern>/deleteCompany</url-pattern>
	</servlet-mapping>



	<welcome-file-list>
		<welcome-file>registerCompany.jsp</welcome-file>
	</welcome-file-list>
</web-app>
